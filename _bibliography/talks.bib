@misc{2018SIAMIS,
    selected = {true},
    author = {},
    title = {Collaborative regularization models for color imaging problems},
    address = {SIAM Conference on Imaging Sciences},
    date = {Bologna, 08/06/2018},
    year = {2018},
    abbr = {Conference},
    abstract = {By considering the gradient of a multichannel image as a 3D tensor with dimensions corresponding to the image domain, spatial derivatives and channels, we introduce collaborative sparsity enforcing norms to address the ill-posedness of color imaging problems. We obtain a regularization framework for color images that uses different channel couplings and enables joint directions of smoothing. We analyze which of the arising models are best suited for color image denoising and other inverse problems.},
    website = {https://www.siam-is18.dm.unibo.it/},
    slides = {2018-conf-SIAMIS.pdf}
}

