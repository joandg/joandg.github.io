@misc{2018Hotelbeds,
    selected = {true},
    author = {},
    title = {Deep learning: Last trends and applications},
    address = {Ciclo de Conferencias Datos Masivos e Inteligencia Artificial, Cátedra Hotelbeds Group & UIB, Palma},
    date = {25/01/2018},
    year = {2018},
    abbr = {Conference},
    abstract = {El objetivo de esta charla es proporcionar una visión general y estimulante de la investigación de vanguardia en técnicas de aprendizaje profundo (Deep Learning) y su uso en varios campos de la inteligencia artificial. Por un lado, se introducirán los conceptos básicos y la historia reciente del aprendizaje profundo, así como los recursos disponibles para el desarrollo de aplicaciones. Y por otro, se analizarán los últimos trabajos más destacados a nivel mundial en esta disciplina, en campos tan diversos como la visión por ordenador; la robótica; los vehículos autónomos; el análisis, descripción escenográfica y clasificación de imágenes y vídeos; o la comprensión del lenguaje humano. Por último, se presentarán algunos de los trabajos de investigación que actualmente se están desarrollando en el Dpto. de Ciencias Matemáticas e Informática que están utilizando aprendizaje profundo.},
    slides = {2018-conf-hotelbeds.pdf}
}
